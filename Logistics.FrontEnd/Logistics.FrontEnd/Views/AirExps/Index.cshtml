
@{
    ViewData["Title"] = "Index";
}

<header>
    <link href="~/css/all/bootstrap.css" rel="stylesheet">
    <link href="~/css/all/normalize.css" rel="stylesheet">
    <link href="~/css/all/animate.css" rel="stylesheet">
    <link href="~/css/style-ll.css" rel="stylesheet">
    <link href="~/css/iconmoon.css" rel="stylesheet">
    <link href="~/css/rwd.css" rel="stylesheet">
    <link href="~/css/print.css" rel="stylesheet">
</header>

<main class="out-main">
    <section class="out-bar-top">
        <i class="mdi mdi-truck-fast"></i>
        <ul class="out-bar-line">
            <li><a href="#" title="">Exports</a></li>
            <li><a href="#" title="">Air Export</a></li>
        </ul>
    </section>
</main>
<div class="col-sm-12 col-md-12 col-lg-12">
    <div class="out-input">
        <label style="display: block">&nbsp;</label>
        <a href="airexps/create" class="btn pull-right"><i class="mdi mdi-plus-circle-outline"></i>&nbsp;Add</a>
    </div>
</div>

<div class="out-content-mid-table">
    <div class="container-fluid">
        <div class="row">
            <section class="col-sm-12 col-md-12 col-lg-12">
            <table id="example" class="display nowrap" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Job ID</th>
                        <th>ETD</th>
                        <th>Carrier</th>
                        <th>Agent</th>
                        <th>Routing</th>
                        <th>Quantity</th>
                        <th>GW</th>
                        <th>Cg</th>
                    </tr>
                </thead>
            </table>
              
            </section>
        </div>
    </div>
</div>

@section Scripts
{
    <script src="~/js/airexps.js"></script>
}

